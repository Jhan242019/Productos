@{
    // List<Account> accounts = ((List<Account>)ViewBag.Accounts); // forma A
    List<Producto> product = ((List<Producto>)Model); // forma B
}

<h1 class="h-100 font-weight-light">Productos</h1>
<a href="/product/create" class="badge badge-success">Registrar Producto</a>

<table class="table table-dark">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Categoria</th>
            <th>Proveedor</th>
            <th>Costo</th>
            <th></th>
        </tr>
    </thead>

    <tbody>


        @for (int i = 0; i < product.Count; i++)
        {
            Producto item = product[i];
            <tr>
                <td>@item.Nombre</td>
                <td>@item.Categoria</td>
                <td>@item.Proveedor</td>
                <td>S/. @item.Costo</td>
                <td>
                    <a href="/Product/edit?id=@item.Id" class="badge badge-warning">Editar</a> |
                    <a href="/Product/delete?id=@item.Id" class="badge badge-danger">Eliminar</a>
                </td>
            </tr>
        }
    </tbody>
</table>    